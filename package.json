{
  "name": "kidsloop-auth-frontend",
  "version": "2.10.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "check:types": "tsc --noEmit",
    "check:eslint": "eslint . --ext .ts,.js,.tsx,.jsx",
    "check": "npm run check:types && npm run check:eslint",
    "start": "cross-env NODE_ENV=development webpack serve",
    "lint": "eslint . --fix --ext .ts,.js,.tsx,.jsx",
    "test": "jest",
    "preversion": "git checkout dev && git pull origin dev && npm i",
    "postversion": "git push && git push --tags",
    "test:sso:default": "cypress run --config-file=cypress/configs/cypress.sso.json --browser chrome --spec \"**/*.feature\"",
    "test:sso:iphone": "cypress run --config-file=cypress/configs/cypress.sso.json --browser chrome --env VIEWPORT=iphone-6 --spec \"**/*.feature\" ",
    "test:sso:ipad": "cypress run --config-file=cypress/configs/cypress.sso.json --browser chrome --env VIEWPORT=ipad-mini --spec \"**/*.feature\" ",
    "test:sso:samsung:note": "cypress run --config-file=cypress/configs/cypress.sso.json --browser chrome --env VIEWPORT=samsung-note9 --spec \"**/*.feature\" ",
    "test:sso:samsung-s10": "cypress run --config-file=cypress/configs/cypress.sso.json --browser chrome --env VIEWPORT=samsung-s10 --spec \"**/*.feature\" ",
    "test:alpha:default": "cypress  run -e TAGS=\"@alpha\" --config-file=cypress/configs/cypress.alpha.json --browser chrome --spec \"**/*.feature\" ",
    "e2e:reports": "node cypress/cucumber-html-report.js",
    "graphql:codegen": "graphql-codegen --config graphql-codegen.yml"
  },
